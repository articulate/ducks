{
  "name": "@articulate/ducks",
  "version": "0.0.0",
  "description": "Collection of redux helper functions",
  "main": "index.js",
  "repository": "git@github.com:articulate/ducks.git",
  "author": "articulate",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "scripts": {
    "build": "buble -i src -o lib",
    "clean": "rm -rf lib && mkdir -p lib",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src test",
    "postversion": "git push --tags origin master",
    "prebuild": "yarn clean",
    "pretest": "yarn build",
    "pretest:coverage": "yarn build",
    "preversion": "git checkout master && yarn build",
    "test": "mocha test --reporter=dot",
    "test:ci": "yarn lint && yarn test:coverage && yarn coverage",
    "test:coverage": "nyc yarn test"
  },
  "dependencies": {
    "crocks": "^0.7.0"
  },
  "devDependencies": {
    "@articulate/spy": "^0.0.1",
    "buble": "^0.15.2",
    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "eslint": "^4.5.0",
    "mocha": "^3.5.0",
    "nyc": "^11.2.1",
    "prop-factory": "^1.0.0",
    "ramda": "^0.24.1"
  }
}
